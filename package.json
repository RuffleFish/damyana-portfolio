{
	"name": "11ty-portfolio",
	"version": "1.0.0",
	"description": "Minimal Poftfolio / blog template built with eleventy & tailwindcss.",
	"main": "index.js",
	"scripts": {
		"clean": "rm -rf public src/css/styles.css",
		"serve": "postcss src/css/tailwind.css > src/css/styles.css && eleventy --serve",
		"build": "npm run clean && NODE_ENV=production postcss src/css/tailwind.css > src/css/styles.css && eleventy"
	},
	"keywords": [
		"11ty",
		"tailwindcss",
		"eleventy",
		"purgecss",
		"cssnano"
	],
	"author": "kailoon",
	"license": "ISC",
	"devDependencies": {
		"@11ty/eleventy": "^0.11.1",
		"@11ty/eleventy-navigation": "^0.1.6",
		"autoprefixer": "^10.2.3",
		"cssnano": "^4.1.10",
		"markdown-it": "^10.0.0",
		"markdown-it-link-attributes": "^3.0.0",
		"postcss-cli": "^8.3.1",
		"tailwindcss": "^2.0.2"
	}
}
